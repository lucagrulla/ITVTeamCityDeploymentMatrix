<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-autowire="constructor">

    <bean id="deployedBuildsController" class="itv.DeployedBuildsController" init-method="register"/>
      <bean id="deployedBuildsLink"
        class="jetbrains.buildServer.web.openapi.SimplePageExtension"
        init-method="register">
        <property name="placeId" value="ALL_PAGES_FOOTER"/>
        <property name="pluginName" value="deployedBuilds"/>
        <property name="includeUrl" value="/plugins/pipeline/deployedBuilds.jsp"/>
      </bean>

</beans>
